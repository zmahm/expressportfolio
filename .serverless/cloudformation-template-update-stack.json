        "CodeSha256": "SNz3ZVf8dV8uU9kaB45XEVogVzwXo+wXpeJsqOHwptQ="
      }
    },
    "HttpApi": {
      "Type": "AWS::ApiGatewayV2::Api",
      "Properties": {
        "Name": "production-my-express-api",
        "ProtocolType": "HTTP"
      }
    },
    "HttpApiStage": {
      "Type": "AWS::ApiGatewayV2::Stage",
